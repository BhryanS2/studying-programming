
void SelectionSort(char vet[], int tam)
{
  int i, j, min;
  char aux;
  for (i = 0; i < (tam - 1); i++)
  {
    min = i;
    for (j = (i + 1); j < tam; j++)
    {
      if (vet[j] < vet[min])
      {
        min = j;
      }
    }
    if (i != min)
    {
      aux = vet[i];
      vet[i] = vet[min];
      vet[min] = aux;
    }
  }
}
